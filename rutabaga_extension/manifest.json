{
  "name": "Rutabaga Extension",
  "version": "1.0",
  "description": "La première extension qui t'aide à protéger la planète",
  "permissions": ["storage"],
  "chrome_url_overrides" : {
      "newtab": "override_page.html"
    },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  "icons": {
      "16": "images/rutabaga_16.png",
      "32": "images/rutabaga_32.png",
      "48": "images/rutabaga_48.png",
      "128": "images/rutabaga_128.png"
    },
  "manifest_version": 2
}
